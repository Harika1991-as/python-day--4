n=int(input("enter the cost of vehicle"))
if(n>100000):
    a=(n*20)/100
    print("the total tax :",a)
elif(n>75000 and n<=10000):
    a=(n*15)/100
    print("the total tax :",a)
elif(n>50000 and n<=75000):
    a=(n*10)/100
    print("the total tax :",a)
elif(n<=50000):
    a=(n*5)/100
    print("the total tax :",a)
